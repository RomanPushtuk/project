{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended",
        "eslint-config-prettier"
    ],
    "plugins": [
        "eslint-plugin-prettier"
    ],
    "rules": {
        "no-unused-vars": "error",
        "no-restricted-imports": "off",
        "prettier/prettier": "error"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "ignorePatterns": ["*.js", "*.json", "*.test.ts"]
}